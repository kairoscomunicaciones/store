{
    "name": "mr-apps/prestashop-module-cleaner",
    "description": "Cleans a prestashop module starting from a json describing the module tree",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "MrAPPs\\ModuleCleaner\\": "src/"
        },
        "exclude-from-classmap": []
    },
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false,
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "type": "prestashop-module",
    "author": "Mr APPs srl",
    "license": "Mr. APPs"
}
